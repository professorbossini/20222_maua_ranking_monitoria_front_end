      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "BACKEND_URL",
      "ENDPOINT_PREPROG",
      "ENDPOINT_TECHINORACLE"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
